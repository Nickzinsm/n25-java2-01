package br.com.senaisp.bauru.secao03.aula07;

public class Parsing01 {

	public static void main(String[] args) {

		int shirtPrice = Integer.parseInt("15");
		double taxRate = Double.parseDouble("0.05");
		String gibbersh = "887dsnds87dsfs";
		//Mostrando os valores
		System.out.println("Preço da camiseta: " + shirtPrice);
		System.out.println("Percential imposto: " + taxRate);
		
		//exemplos de erros de conversão abaixo!
		System.out.println("gibberish para int :" + Integer.parseInt("0.05"));
		System.out.println("gibberish para int :" + Integer.parseInt(gibbersh));
	}

}
