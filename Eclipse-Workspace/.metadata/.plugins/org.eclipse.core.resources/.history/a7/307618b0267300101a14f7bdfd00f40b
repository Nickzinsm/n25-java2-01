package br.com.senaisp.bauru.secao03.aula07;

import java.util.Scanner;

public class Exemplo05 {

	public static void main(String[] args) {

 		Scanner sc = new Scanner(Exemplo05.class.getResourceAsStream("cadastros.csv"));
 		//Ttrocando o token do scanner
 		sc.useDelimiter(",");
 		//Imprimindo o cabecalho
 		for(int i=0;i<4;i++) {
 			//cuidado que é print e não println
 			System.out.print(sc.next().replace("\"","") + "\t");
 		}
 		System.out.println(); //pulando a linha
 		sc.close();
	}

}
